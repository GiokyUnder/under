    min-height: 550px;
    background-image: -webkit-gradient(
        linear,
        left bottom,
        left top,
        from(#222),
        to(rgba(34, 34, 34, 0))
      ),
      url(/src/assets/img/main.webp);
    background-image: linear-gradient(360deg, #222, rgba(34, 34, 34, 0)),
      url(/src/assets/img/main.webp);
    background-position:
      0 0,
      50% 50%;
    background-size: auto, cover;
  }
