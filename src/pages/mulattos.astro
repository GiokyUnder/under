---
import Layout from "../layouts/Layout.astro";
import ModelCard from "../components/ModelCard.astro";
import { mulattoModels } from "./models/data/mulattos";
---

<Layout>
  <meta
    slot="description"
    name="description"
    content="Заказывайте мулаток эскортниц от агентства Aurora Escort в Москве. Полная анонимность! Эскортницы VIP класса, привлекательные девушки."
  />
  <title slot="title">Мулатки эскортницы в Москве | Aurora Escort</title>
  <meta slot="meta-noindex" name="robots" content="noindex, follow" />

  <div class="bg-white">
    <div
      class="mx-auto max-w-2xl px-4 py-16 sm:px-6 sm:py-24 lg:max-w-7xl lg:px-8"
    >
      <h1 class="text-stone-600">Мулатки эскортницы в Москве</h1>
      <div class="flex items-center justify-center py-4 flex-wrap mt-10">
        <a
          href="/brunnetes"
          type="button"
          class="text-gray-900 border border-gray-200 bg-white ring-4 ring-gray-300 rounded-full text-base font-medium px-5 py-2.5 text-center me-3 mb-3 focus:ring-gray-500 hover:ring-red-300 hover:border-red-300"
          >Брюнетки</a
        >
        <a
          href="/blonds"
          type="button"
          class="text-gray-900 border border-gray-200 bg-white ring-4 ring-gray-300 rounded-full text-base font-medium px-5 py-2.5 text-center me-3 mb-3 focus:ring-gray-500 hover:ring-red-300 hover:border-red-300"
          >Блондинки</a
        >
        <a
          href="/redhead"
          type="button"
          class="text-gray-900 border border-gray-200 bg-white ring-4 ring-gray-300 rounded-full text-base font-medium px-5 py-2.5 text-center me-3 mb-3 focus:ring-gray-500 hover:ring-red-300 hover:border-red-300"
          >Рыжие</a
        >
        <a
          href="/feet"
          type="button"
          class="text-gray-900 border border-gray-200 bg-white ring-4 ring-gray-300 rounded-full text-base font-medium px-5 py-2.5 text-center me-3 mb-3 focus:ring-gray-500 hover:ring-red-300 hover:border-red-300"
          >Длинные ноги</a
        >
        <a
          href="/youth"
          type="button"
          class="text-gray-900 border border-gray-200 bg-white ring-4 ring-gray-300 rounded-full text-base font-medium px-5 py-2.5 text-center me-3 mb-3 focus:ring-gray-500 hover:ring-red-300 hover:border-red-300"
          >Молоденькие</a
        >
        <a
          href="/skinny"
          type="button"
          class="text-gray-900 border border-gray-200 bg-white ring-4 ring-gray-300 rounded-full text-base font-medium px-5 py-2.5 text-center me-3 mb-3 focus:ring-gray-500 hover:ring-red-300 hover:border-red-300"
          >Худые</a
        >
        <a
          href="/asian"
          type="button"
          class="text-gray-900 border border-gray-200 bg-white ring-4 ring-gray-300 rounded-full text-base font-medium px-5 py-2.5 text-center me-3 mb-3 focus:ring-gray-500 hover:ring-red-300 hover:border-red-300"
          >Азиатки</a
        >
      </div>

      <div
        class="grid grid-cols-1 gap-x-6 gap-y-10 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 xl:gap-x-8"
      >
        {
          mulattoModels.map(
            ({ props: { name, price, images }, params: { id } }) => (
              <ModelCard id={id} name={name} price={price} img={images[0]} />
            )
          )
        }
      </div>
    </div>
  </div>
</Layout>
